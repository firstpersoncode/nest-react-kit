{
    "name": "nest-react-kit",
    "version": "0.0.1",
    "description": "NestJS + ReactJS",
    "author": "Nasser Maronie <nasser.maronie@gmail.com>",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "start-docker:dev": "docker-compose -f local.docker-compose.yml up -d && docker-compose -f local.docker-compose.yml logs -f",
        "start-docker:staging": "docker-compose -f staging.docker-compose.yml up -d",
        "start-docker:production": "docker-compose -f production.docker-compose.yml up -d",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/orm.config.ts",
        "typeorm:migrate": "npm run typeorm migration:generate -- -n",
        "typeorm:run": "npm run typeorm migration:run",
        "lint": "eslint \"{src,apps,libs,test,views}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./jest-e2e.json",
        "start:gulp": "gulp"
    },
    "dependencies": {
        "@nestjs-modules/mailer": "^1.3.12",
        "@nestjs/common": "^6.10.14",
        "@nestjs/core": "^6.10.14",
        "@nestjs/graphql": "^6.6.2",
        "@nestjs/platform-express": "^6.10.14",
        "@nestjs/typeorm": "^7.0.0",
        "apollo-server-express": "^2.11.0",
        "bcryptjs": "^2.4.3",
        "cache-manager": "^3.2.1",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.11.0",
        "cookie-parser": "^1.4.5",
        "cross-env": "^7.0.2",
        "dotenv": "^8.2.0",
        "graphql": "^14.6.0",
        "graphql-tools": "^4.0.7",
        "json-2-csv": "^3.6.1",
        "jsonwebtoken": "^9.0.0",
        "mime-types": "^2.1.26",
        "pg": "^7.18.2",
        "pug": "^2.0.4",
        "reflect-metadata": "^0.1.13",
        "remove": "^0.1.5",
        "rimraf": "^3.0.0",
        "rxjs": "^6.5.4",
        "shortid": "^2.2.15",
        "type-graphql": "^0.17.6",
        "typeorm": "^0.2.24"
    },
    "devDependencies": {
        "@babel/plugin-transform-react-jsx": "^7.9.4",
        "@babel/plugin-transform-runtime": "^7.9.0",
        "@babel/preset-env": "^7.8.7",
        "@babel/preset-react": "^7.9.4",
        "@babel/preset-typescript": "^7.9.0",
        "@date-io/core": "^1.3.13",
        "@date-io/date-fns": "^1.3.13",
        "@material-ui/core": "^4.9.7",
        "@material-ui/lab": "^4.0.0-alpha.46",
        "@material-ui/pickers": "^3.2.10",
        "@nestjs/cli": "^6.13.2",
        "@nestjs/schematics": "^6.8.1",
        "@nestjs/testing": "^6.10.14",
        "@types/bcryptjs": "^2.4.2",
        "@types/cookie-parser": "^1.4.2",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.2",
        "@types/jest": "25.1.2",
        "@types/jsonwebtoken": "^8.3.8",
        "@types/node": "^13.1.6",
        "@types/react-redux": "^7.1.7",
        "@types/shortid": "^0.0.29",
        "@types/supertest": "^2.0.8",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "@typescript-eslint/parser": "^2.12.0",
        "apollo-server-testing": "^2.11.0",
        "babelify": "^10.0.0",
        "browserify": "^16.5.0",
        "date-fns": "^2.11.1",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^2.5.0",
        "fancy-log": "^1.3.3",
        "gulp": "^4.0.2",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-typescript": "^6.0.0-alpha.1",
        "gulp-uglify": "^3.0.2",
        "jest": "^24.9.0",
        "prettier": "^1.18.2",
        "react": "^16.13.0",
        "react-copy-to-clipboard": "^5.0.2",
        "react-dom": "^16.13.0",
        "react-redux": "^7.2.0",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-thunk": "^2.3.0",
        "supertest": "^4.0.2",
        "ts-jest": "25.2.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.7.4",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0",
        "watchify": "^3.11.1"
    }
}
